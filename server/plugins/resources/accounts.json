{
  "model": "Account",
  "acl": {
    "mobileNumber": {
      "get?_id": ["showUserMobilePhone"],
      "post": "*",
      "put?_id": ["showUserMobilePhone"]
    },
    "username": {
      "get": "*",
      "post": "*",
      "get?_id": "*",
      "put?_id": false
    },
    "activationToken": false,
    "__v": false,
    "*": {
      "get": "*",
      "get?_id": "*",
      "put?_id": "*",
      "post": "*",
      "delete?_id": "*"
    }
  }
}
